e\n    };\n    db.settings(settings);\n\nexport class Message extends React.Component{\n    constructor(props) {\n        // this.handleSubmit = this.handleSubmit.bind(this);\n        super(props);\n        this.state = {\n            fieldCount: 1,\n            name: \"\",\n            email: \"\",\n            phone: \"\",\n            message: \"\",\n        }\n        const metadata = {\n            timestamp: new Date(),\n            clientID: \"1\",\n            recipient: \"Richard\",\n            docRef: db.doc(\"Users/\" + this.recipient + \"/Messages/\" + this.autoID(10) )\n        }\n        \n        this.handleAddField = this.handleAddField.bind(this);\n    }\n    // Initialize Cloud Firestore through Firebase\n    \n    handleChange(event) {\n        this.setState(\n            {name: e.target.value}\n        );\n    }\n\n    handleAddField(){\n        this.setState(\n            {fieldCount: this.state.fieldCount + 1}\n        )\n        \n        console.log(this.state.fieldCount);\n    }\n\n    autoID(length) {\n        const chars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\n        let autoId = '';\n        for (let i = 0; i < length; i++) {\n            autoId += chars.charAt(Math.floor(Math.random() * chars.length))\n        }\n        console.log(\"autoID: \"+autoId);\n        return autoId;\n    }\n    \n\n\n    generateFields(){\n        let i;\n        \n        return this.fields\n    }\n\n    render(){\n        console.log()\n        let fields = []\n        /* ['Name', 'text'], ['Phone', 'text'], ['E-mail', 'email'] */\n        for( let i = 0; this.state.fieldCount <= i; i++){\n            this.fields.push(<TextField/>);\n            console.log(this.fields)\n        }\n        return (\n            <section id=\"message\">\n                {this.fields}\n                <button id=\"submit\" onClick={this.handleAddField}> Next</button>\n            </section>\n        )\n    }\n}\n\n// componentDidCatch(error, info) {\n//     // Display fallback UI\n//     this.setState({ hasError: true });\n//     // You can also log the error to an error reporting service\n//     logErrorToMyService(error, info);\n// }\n\n// value(element){\n//     console.log(\"Parsing form data\")\n//     return document.querySelector(element).value;\n// }\n\n// send() {\n//     console.log(\"Sending message\")\n//     this.docRef.set({\n//         name: this.name,\n//         message: this.message,\n//         email: this.email,\n//         phone: this.phone,\n//         date: this.date,\n//         clientID: this.clientID\n//     }).then(function() {\n//         console.log(\"Document successfully written!\");\n//         return () => true;\n//     }).catch(function(error){\n//         console.error(\"Error writing document: \" + error);\n//         return () => false;\n//     })\n// }"},"lineCount":null}},"hash":"f2c6af8666947368b7c4d44b10af91a4","cacheData":{"env":{}}}